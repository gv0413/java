		Song s1 = new Song();
//		s1.setMode("NonDiscounted");
		Song s2 = new Song();
//		s2.setMode("NonDiscounted");
		Song s3 = new OnSale();
//		s3.setMode("OnSale");
		Song s4 = new TodayEvent();
//		s4.setMode("TodayEvent");
		
